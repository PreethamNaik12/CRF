<p id="notice"><%= notice %></p>
<%= stylesheet_link_tag "announcement"%> <!-- this has to be removed during code optimization-->
<%= stylesheet_link_tag "loginFeatures"%> <!-- this has to be removed during code optimization-->
<%= stylesheet_link_tag "adminportal"%> <!-- this has to be removed during code optimization-->

<%=render "partials/feedbackNav"%>

<div id="container">
 <table class="table table-hover font-quicksand">
  <thead id="thead">
    <tr class="table-head font-unbounded">
      <th scope="col" >Feedback Id</th>
      <th scope="col" >Username</th>
      <th scope="col" >Email</th>
      <th scope="col" >Feedback</th>
      <th scope="col" >Date</th>
      <th scope="col" >View</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.id %></td>
        <td><%= feedback.username %></td>
        <td><%= feedback.email %></td>
        <td><%= feedback.feedback %></td>
        <td><%= feedback.date %></td>
        <td><%= link_to 'Show', feedback , class:"btn btn-primary" %></td>

        <td><%#<%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%#<%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

